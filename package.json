    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdom": "^11.1.0",
    "jszip": "^3.1.3",
    "localforage": "^1.5.0",
    "lodash.isequal": "^4.5.0",
    "material-ui": "^0.18.6",
